<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import MotsApp.*?>
<?import MotsApp.Modèles.*?>
<?scenebuilder-classpath-element .?>

<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="750.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" 
            fx:controller="MotsApp.Contrôleurs.ArticleRechercheResultatsContrôleur">
  <Label fx:id="resultatsRechercheLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="0.0" layoutY="120.0" prefWidth="750.0" style="-fx-font: NORMAL 20 Tahoma;" text="Resultats de la Recherche" textAlignment="CENTER" />
  <GridPane alignment="CENTER" hgap="10.0" layoutX="44.0" layoutY="174.0" prefHeight="367.0" prefWidth="662.0" vgap="10.0">
    <TableView fx:id="matièreTableView" prefHeight="367.0" prefWidth="662.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="100.0" text="Titre" fx:id="matièreTitre">
          <cellValueFactory>
            <PropertyValueFactory property="titre" />
          </cellValueFactory>
        </TableColumn>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="100.0" text="Auteur" fx:id="matièreAuteur">
          <cellValueFactory>
            <PropertyValueFactory property="auteur" />
          </cellValueFactory>
        </TableColumn>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="181.0" text="Contenu" fx:id="matièreContenu">
          <cellValueFactory>
            <PropertyValueFactory property="contenu" />
          </cellValueFactory>
        </TableColumn>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="80.0" text="Date" fx:id="matièreDate">
          <cellValueFactory>
            <PropertyValueFactory property="date" />
          </cellValueFactory>
        </TableColumn>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="180.0" text="Source" fx:id="matièreSource">
          <cellValueFactory>
            <PropertyValueFactory property="source" />
          </cellValueFactory>
        </TableColumn>
      </columns>
      <items>
        <FXCollections fx:factory="observableArrayList" />
        <!--<FXCollections fx:factory="observableArrayList">
          <Article titre="default_titre1" auteur="default_auteur1" contenu="default_contenu1" />
          <Article titre="default_titre2" auteur="default_auteur2" contenu="default_contenu2" />
          <Article titre="default_titre3" auteur="default_auteur3" contenu="default_contenu3" />
        </FXCollections>-->
      </items>
    </TableView>
    <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
  </GridPane>
</AnchorPane>
